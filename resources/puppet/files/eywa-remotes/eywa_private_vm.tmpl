NAME="@@__UID__@@-Ubuntu(EYWA)"
CONTEXT=[
	ONE_UID="@@__UID__@@",
	IS_EYWA="yes",
	IS_VR="no",
	FILES="/var/lib/one/files/eywa-vm/init.sh",
	PASSWD="$USER[ROOT_PW]",
	SSH_PUBLIC_KEY="$USER[SSH_PUBLIC_KEY]",
	ETH0_IP="$NIC[IP,NETWORK=\"@@__UID__@@-Private-Net\"]"
]

CPU="1"

DISK=[IMAGE="Ubuntu12.04_64",IMAGE_UID="0"]

FEATURES=[ACPI="yes"]

GRAPHICS=[LISTEN="0.0.0.0",TYPE="VNC"]

MEMORY="512"

NIC=[MODEL="virtio",NETWORK="@@__UID__@@-Private-Net",NETWORK_UID="0"]

OS=[ARCH="x86_64",BOOT="hd"]
