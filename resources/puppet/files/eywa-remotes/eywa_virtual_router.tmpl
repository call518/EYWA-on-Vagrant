NAME="@@__UID__@@-EYWA-Router"
CONTEXT=[
	ONE_UID="@@__UID__@@",
	IS_EYWA="yes",
	IS_VR="yes",
	FILES="/var/lib/one/files/eywa-vr/init.sh /var/lib/one/files/eywa-vr/iptables.rules /var/lib/one/files/eywa-vr/haproxy-cfg-gen /var/lib/one/files/eywa-vr/haproxy.cfg.tmpl /var/lib/one/files/eywa-vr/haproxy.init /var/lib/one/.ssh",
	PASSWD="1234",
	SSH_PUBLIC_KEY="@@__SSH_PUB_KEY__@@",
	ETH0_IP="$NIC[IP,NETWORK=\"Public-Net\"]",
    ETH0_GATEWAY="192.168.33.2",
	ETH1_IP="$NIC[IP,NETWORK=\"@@__UID__@@-Private-Net\"]",
    HAPROXY_CFG="http:0.0.0.0:8080:80:1000:100:10.0.0.1,10.0.0.2,10.0.0.3,10.0.0.4,10.0.0.5,10.0.0.6,10.0.0.7,10.0.0.8,10.0.0.9,10.0.0.10"
]
CPU="0.2"
VCPU="2"
DISK=[IMAGE="EYWA-Virtual-Router",IMAGE_UNAME="oneadmin"]
FEATURES=[ACPI="yes"]
GRAPHICS=[LISTEN="0.0.0.0",TYPE="VNC"]
MEMORY="256"
NIC=[MODEL="virtio",NETWORK="Public-Net",NETWORK_UNAME="oneadmin"]
NIC=[MODEL="virtio",NETWORK="@@__UID__@@-Private-Net",NETWORK_UNAME="oneadmin"]
OS=[ARCH="x86_64",BOOT="hd"]
