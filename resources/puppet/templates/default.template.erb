NAME="Ubuntu-14.04"
CONTEXT=[
  ETH0_IP="$NIC[IP,NETWORK=\"Public-Net\"]",
  ETH0_MAC="$NIC[MAC,NETWORK=\"Public-Net\"]",
  ETH0_GATEWAY="192.168.33.2",
  ETH0_NETWORK="192.168.33.0",
  ETH0_MASK="255.255.255.0",
  GATEWAY_IFACE_NUM="0",
  DNS="8.8.8.8",
  FILES="/var/lib/one/files/public-vm/init.sh",
  IS_EYWA="no",
  IS_VR="no",
  PASSWD="<%= @vm_root_pw %>",
  SSH_PUB_KEY="@@__SSH_PUB_KEY__@@" ]
CPU="1"
DISK=[
  IMAGE="Ubuntu-14.04",
  IMAGE_UNAME="oneadmin" ]
FEATURES=[
  ACPI="yes" ]
GRAPHICS=[
  LISTEN="0.0.0.0",
  TYPE="VNC" ]
MEMORY="256"
NIC=[
  NETWORK="Public-Net",
  NETWORK_UNAME="oneadmin" ]
OS=[
  ARCH="x86_64",
  BOOT="hd" ]
